{
  "name": "qplayer",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Simple and functional music player for Chrome",
  "permissions": [
    "http://code.jquery.com/jquery-1.9.1.min.js",
    {"mediaGalleries": ["read", "allAutoDetected"]},
    "notifications",
    "storage"
  ],
  "icons": {
    "16": "img/logo_16.png",
    "48": "img/logo_48.png",
    "128": "img/logo_128.png"
  },
  "app": {
     "background": {
       "scripts": ["app.js"]
     }
  },
  "offline_enabled": true
}
